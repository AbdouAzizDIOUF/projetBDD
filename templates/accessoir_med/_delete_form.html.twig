<form method="post" action="{{ path('accessoir_med_delete', {'id': accessoir_med.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ accessoir_med.id) }}"><br>
    <button class="btn btn-danger">Delete</button>
</form>
