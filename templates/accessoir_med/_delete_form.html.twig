<form method="post" action="{{ path('accessoir_med_delete', {'id': accessoir_med.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cet accessoire medical?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ accessoir_med.id) }}"><br>
    <button class="btn btn-danger">supprimer</button>
</form>
